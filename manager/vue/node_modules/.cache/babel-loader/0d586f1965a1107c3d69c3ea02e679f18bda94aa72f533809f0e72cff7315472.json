{"ast":null,"code":"import router from \"@/router\";\nexport default {\n  data() {\n    return {\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\n      carouseData: [require('@/assets/imgs/lun-1.jpg'), require('@/assets/imgs/lun-2.jpg'), require('@/assets/imgs/lun-3.jpg')],\n      lostData: [],\n      foundData: [],\n      viewVisible: false,\n      viewData: null,\n      fromVisible: false,\n      content: null,\n      form: {}\n    };\n  },\n  mounted() {\n    this.loadLostData();\n    this.loadFoundData();\n  },\n  // methods：本页面所有的点击事件或者其他函数定义区\n  methods: {\n    router() {\n      return router;\n    },\n    loadLostData() {\n      this.$request.get('/lost/selectNew4').then(res => {\n        if (res.code === '200') {\n          this.lostData = res.data;\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    loadFoundData() {},\n    viewContent(content) {\n      this.viewData = content;\n      this.viewVisible = true;\n    },\n    contact(item) {\n      this.form = JSON.parse(JSON.stringify(item));\n      this.fromVisible = true;\n    },\n    submit() {\n      if (this.user.id === this.form.userId) {\n        this.$message.warning('您不能联系自己');\n        this.content = null;\n        return;\n      }\n      let data = {\n        articleId: this.form.id,\n        type: '失物广场',\n        fromId: this.user.id,\n        toId: this.form.userId,\n        content: this.content\n      };\n      this.$request.post('/message/add', data).then(res => {\n        if (res.code === '200') {\n          this.$message.success('留言成功，等待对方联系');\n          this.content = null;\n          this.fromVisible = false;\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["router","data","user","JSON","parse","localStorage","getItem","carouseData","require","lostData","foundData","viewVisible","viewData","fromVisible","content","form","mounted","loadLostData","loadFoundData","methods","$request","get","then","res","code","$message","error","msg","viewContent","contact","item","stringify","submit","id","userId","warning","articleId","type","fromId","toId","post","success"],"sources":["src/views/front/Home.vue"],"sourcesContent":["<template>\r\n  <div class=\"main-content\">\r\n    <div style=\"width: 80%; margin: 0 auto\">\r\n      <el-carousel height=\"350px\">\r\n        <el-carousel-item v-for=\"item in carouseData\" :key=\"item\">\r\n          <img :src=\"item\" alt=\"\" style=\"width: 100%; height: 350px\">\r\n        </el-carousel-item>\r\n      </el-carousel>\r\n      <div style=\"margin-top:20px\">\r\n        <el-row :gutter=\"20\">\r\n          <el-col :span=\"12\">\r\n            <div class=\"card\" >\r\n              <div style=\"display: flex\">\r\n                <div style=\"flex: 1;color: #725b04; font-size: 18px;font-weight: 550 \">最新发布失物</div>\r\n                <div style=\"flex: 1; text-align: right;color: #666666;cursor:pointer\"@click=\"$router.push('/front/lostSquare')\">查看更多</div>\r\n              </div>\r\n              <div style=\"margin-top: 40px\">\r\n                <el-row :gutter=\"20\">\r\n                  <el-col :span=\"12\" v-for=\"item in lostData\" style=\"margin-bottom:20px\" >\r\n                    <div class=\"transition card\">\r\n                      <div style=\"display: flex\" >\r\n                        <img :src=\"item.img\" alt=\"\" style=\"height: 75px; width: 75px; border: 1px solid #eeeeee; border-radius: 10px \">\r\n                        <div style=\"margin-left: 10px\">\r\n                          <div style=\"font-weight: 550;color: #ef5d26\">{{item.name}}</div>\r\n                          <div style=\"margin-top: 10px;color: #666666\">发布人：{{item.userName}}</div>\r\n                          <div style=\"margin-top: 5px;color: #666666\">时间：{{item.time}}</div>\r\n                        </div>\r\n                      </div>\r\n                      <div style=\"margin-top: 20px; text-align: center\">\r\n                        <el-button type=\"info\" @click=\"viewContent(item.content)\">查看详情</el-button>\r\n                        <el-button type=\"success\" @click=\"contact(item)\">联系失主</el-button>\r\n                      </div>\r\n                    </div>\r\n                  </el-col>\r\n                </el-row>\r\n              </div>\r\n            </div>\r\n          </el-col>\r\n          <el-col :span=\"12\">\r\n            <div class=\"card\" >\r\n              <div style=\"display: flex\">\r\n                <div style=\"flex: 1;color: #725b04; font-size: 18px;font-weight: 550 \">最新发布招领</div>\r\n                <div style=\"flex: 1; text-align: right;color: #666666\">查看更多</div>\r\n              </div>\r\n              <div style=\"margin-top: 40px\">\r\n                <el-row :gutter=\"20\">\r\n                  <el-col :span=\"12\" v-for=\"item in foundData\" >\r\n                    <div class=\"transition card\">\r\n                      <div style=\"display: flex\" >\r\n                        <img :src=\"item.img\" alt=\"\" style=\"height: 75px; width: 75px; border: 1px solid #eeeeee; border-radius: 10px \">\r\n                        <div style=\"margin-left: 10px\">\r\n                          <div style=\"font-weight: 550;color: #ef5d26\">{{item.name}}</div>\r\n                          <div style=\"margin-top: 10px;color: #666666\">{{item.userName}}</div>\r\n                          <div style=\"margin-top: 5px;color: #666666\">时间：{{item.time}}</div>\r\n                        </div>\r\n                      </div>\r\n                      <div style=\"margin-top: 20px; text-align: center\">\r\n                        <el-button type=\"info\">查看详情</el-button>\r\n                        <el-button type=\"success\">联系失主</el-button>\r\n                      </div>\r\n                    </div>\r\n                  </el-col>\r\n                </el-row>\r\n              </div>\r\n            </div>\r\n          </el-col>\r\n        </el-row>\r\n      </div>\r\n    </div>\r\n    <el-dialog title=\"详细信息\" :visible.sync=\"viewVisible\" width=\"55%\" :close-on-click-modal=\"false\" destroy-on-close>\r\n      <div v-html=\"viewData\" class=\"w-e-text w-e-text-container\"></div>\r\n    </el-dialog>\r\n    <el-dialog title=\"留言信息\" :visible.sync=\"fromVisible\" width=\"40%\" :close-on-click-modal=\"false\" destroy-on-close>\r\n      <el-form label-width=\"100px\" style=\"padding-right: 50px\"  >\r\n        <el-form-item prop=\"title\" label=\"友情提示\">\r\n          <span style=\"color:red\">请详细描述你的联系方式，方便对方联系到你</span>\r\n        </el-form-item>\r\n        <el-form-item prop=\"content\" label=\"留言内容\">\r\n          <el-input type=\"textarea\" :rows=\"5\" v-model=\"content\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"fromVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"submit\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport router from \"@/router\";\r\n\r\nexport default {\r\n\r\n  data() {\r\n    return {\r\n      user:JSON.parse(localStorage.getItem('xm-user')||'{}'),\r\n      carouseData:[\r\n        require('@/assets/imgs/lun-1.jpg'),\r\n        require('@/assets/imgs/lun-2.jpg'),\r\n        require('@/assets/imgs/lun-3.jpg'),\r\n      ],\r\n      lostData:[],\r\n      foundData:[],\r\n      viewVisible:false,\r\n      viewData:null,\r\n      fromVisible:false,\r\n      content:null,\r\n      form:{}\r\n    }\r\n  },\r\n  mounted() {\r\n    this.loadLostData()\r\n    this.loadFoundData()\r\n  },\r\n  // methods：本页面所有的点击事件或者其他函数定义区\r\n  methods: {\r\n    router() {\r\n      return router\r\n    },\r\n    loadLostData(){\r\n      this.$request.get('/lost/selectNew4').then(res=>{\r\n        if(res.code==='200'){\r\n          this.lostData=res.data\r\n        }else{\r\n          this.$message.error(res.msg)\r\n        }\r\n\r\n      })\r\n    },\r\n    loadFoundData(){\r\n\r\n    },\r\n    viewContent(content){\r\n      this.viewData=content\r\n      this.viewVisible=true\r\n    },\r\n    contact(item){\r\n      this.form=JSON.parse(JSON.stringify(item))\r\n      this.fromVisible=true\r\n    },\r\n    submit() {\r\n      if(this.user.id===this.form.userId){\r\n        this.$message.warning('您不能联系自己')\r\n        this.content=null\r\n        return\r\n      }\r\n      let data = {\r\n        articleId:this.form.id,\r\n        type:'失物广场',\r\n        fromId:this.user.id,\r\n        toId:this.form.userId,\r\n        content:this.content\r\n      }\r\n      this.$request.post('/message/add', data).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success('留言成功，等待对方联系')\r\n          this.content=null\r\n          this.fromVisible=false\r\n        }else{\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n"],"mappings":"AA2FA,OAAAA,MAAA;AAEA;EAEAC,KAAA;IACA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAC,WAAA,GACAC,OAAA,6BACAA,OAAA,6BACAA,OAAA,4BACA;MACAC,QAAA;MACAC,SAAA;MACAC,WAAA;MACAC,QAAA;MACAC,WAAA;MACAC,OAAA;MACAC,IAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,YAAA;IACA,KAAAC,aAAA;EACA;EACA;EACAC,OAAA;IACAnB,OAAA;MACA,OAAAA,MAAA;IACA;IACAiB,aAAA;MACA,KAAAG,QAAA,CAAAC,GAAA,qBAAAC,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAf,QAAA,GAAAc,GAAA,CAAAtB,IAAA;QACA;UACA,KAAAwB,QAAA,CAAAC,KAAA,CAAAH,GAAA,CAAAI,GAAA;QACA;MAEA;IACA;IACAT,cAAA,GAEA;IACAU,YAAAd,OAAA;MACA,KAAAF,QAAA,GAAAE,OAAA;MACA,KAAAH,WAAA;IACA;IACAkB,QAAAC,IAAA;MACA,KAAAf,IAAA,GAAAZ,IAAA,CAAAC,KAAA,CAAAD,IAAA,CAAA4B,SAAA,CAAAD,IAAA;MACA,KAAAjB,WAAA;IACA;IACAmB,OAAA;MACA,SAAA9B,IAAA,CAAA+B,EAAA,UAAAlB,IAAA,CAAAmB,MAAA;QACA,KAAAT,QAAA,CAAAU,OAAA;QACA,KAAArB,OAAA;QACA;MACA;MACA,IAAAb,IAAA;QACAmC,SAAA,OAAArB,IAAA,CAAAkB,EAAA;QACAI,IAAA;QACAC,MAAA,OAAApC,IAAA,CAAA+B,EAAA;QACAM,IAAA,OAAAxB,