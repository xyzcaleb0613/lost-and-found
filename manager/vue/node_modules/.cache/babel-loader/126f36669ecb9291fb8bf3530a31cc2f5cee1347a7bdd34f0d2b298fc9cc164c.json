{"ast":null,"code":"import E from 'wangeditor';\nexport default {\n  data() {\n    return {\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\n      name: null,\n      pageNum: 1,\n      pageSize: 8,\n      total: 0,\n      lostData: [],\n      viewVisible: false,\n      viewData: null,\n      fromVisible: false,\n      content: null,\n      form: {}\n    };\n  },\n  mounted() {\n    this.load(1);\n  },\n  // methods：本页面所有的点击事件或者其他函数定义区\n  methods: {\n    load(pageNum) {\n      if (pageNum) this.pageNum = pageNum;\n      this.$request.get('/lost/selectPage', {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          name: this.name\n        }\n      }).then(res => {\n        this.lostData = res.data?.list;\n        this.total = res.data?.total;\n      });\n    },\n    handleCurrentChange(pageNum) {\n      this.pageNum = pageNum;\n      this.load(this.pageNum);\n    },\n    reset() {\n      this.name = null;\n      this.load(1);\n    },\n    viewContent(content) {\n      this.viewData = content;\n      this.viewVisible = true;\n    },\n    contact(item) {\n      this.form = JSON.parse(JSON.stringify(item));\n      this.fromVisible = true;\n    },\n    submit() {\n      let data = {\n        articleId: this.form.id,\n        type: '失物广场',\n        fromId: this.user.id,\n        toId: this.form.userId,\n        content: this.content\n      };\n      this.$request.post('/message/add', data).then(res => {\n        if (res.code === '200') {\n          this.$message.success('留言成功，等待对方联系');\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["E","data","user","JSON","parse","localStorage","getItem","name","pageNum","pageSize","total","lostData","viewVisible","viewData","fromVisible","content","form","mounted","load","methods","$request","get","params","then","res","list","handleCurrentChange","reset","viewContent","contact","item","stringify","submit","articleId","id","type","fromId","toId","userId","post","code","$message","success","error","msg"],"sources":["src/views/front/LostSquare.vue"],"sourcesContent":["<template>\r\n  <div class=\"main-content\">\r\n    <div style=\"width:70%; margin: 20px auto\">\r\n      <div style=\"color: #8b4513;font-size:16px;font-weight:550\">\r\n        这里是失物广场，失主一定很急，如果您看到了这些物品，请及时联系失主噢！\r\n      </div>\r\n      <div style=\"margin-top: 30px\">\r\n        <el-input placeholder=\"请输入物品名称查询\" style=\"width: 200px\" v-model=\"name\"></el-input>\r\n        <el-button type=\"info\" plain style=\"margin-left: 10px\" @click=\"load(1)\">查询</el-button>\r\n        <el-button type=\"warning\" plain style=\"margin-left: 10px\" @click=\"reset\">重置</el-button>\r\n      </div>\r\n      <div style=\"margin-top: 30px\">\r\n        <el-row :gutter=\"20\">\r\n          <el-col :span=\"6\" v-for=\"item in lostData\" style=\"margin-bottom:20px\" >\r\n            <div class=\"transition card\">\r\n              <div style=\"display: flex\" >\r\n                <img :src=\"item.img\" alt=\"\" style=\"height: 75px; width: 75px; border: 1px solid #eeeeee; border-radius: 10px \">\r\n                <div style=\"margin-left: 10px\">\r\n                  <div style=\"font-weight: 550;color: #ef5d26\">{{item.name}}</div>\r\n                  <div style=\"margin-top: 10px;color: #666666\">发布人：{{item.userName}}</div>\r\n                  <div style=\"margin-top: 5px;color: #666666\">时间：{{item.time}}</div>\r\n                </div>\r\n              </div>\r\n              <div style=\"margin-top: 20px; text-align: center\">\r\n                <el-button type=\"info\" @click=\"viewContent(item.content)\">查看详情</el-button>\r\n                <el-button type=\"success\" @click=\"contact(item)\">联系失主</el-button>\r\n              </div>\r\n            </div>\r\n          </el-col>\r\n        </el-row>\r\n      </div>\r\n      <div class=\"pagination\">\r\n        <el-pagination\r\n            background\r\n            @current-change=\"handleCurrentChange\"\r\n            :current-page=\"pageNum\"\r\n            :page-sizes=\"[5, 10, 20]\"\r\n            :page-size=\"pageSize\"\r\n            layout=\"total, prev, pager, next\"\r\n            :total=\"total\">\r\n        </el-pagination>\r\n      </div>\r\n    </div>\r\n    <el-dialog title=\"详细信息\" :visible.sync=\"viewVisible\" width=\"55%\" :close-on-click-modal=\"false\" destroy-on-close>\r\n      <div v-html=\"viewData\" class=\"w-e-text w-e-text-container\"></div>\r\n    </el-dialog>\r\n    <el-dialog title=\"留言信息\" :visible.sync=\"fromVisible\" width=\"40%\" :close-on-click-modal=\"false\" destroy-on-close>\r\n      <el-form label-width=\"100px\" style=\"padding-right: 50px\"  >\r\n        <el-form-item prop=\"title\" label=\"友情提示\">\r\n          <span style=\"color:red\">请详细描述你的联系方式，方便对方联系到你</span>\r\n        </el-form-item>\r\n        <el-form-item prop=\"content\" label=\"留言内容\">\r\n          <el-input type=\"textarea\" :rows=\"5\" v-model=\"content\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"fromVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"submit\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport E from 'wangeditor'\r\nexport default {\r\n\r\n  data() {\r\n    return {\r\n      user:JSON.parse(localStorage.getItem('xm-user')||'{}'),\r\n      name: null,\r\n      pageNum: 1,\r\n      pageSize: 8,\r\n      total: 0,\r\n      lostData: [],\r\n      viewVisible: false,\r\n      viewData: null,\r\n      fromVisible: false,\r\n      content: null,\r\n      form: {}\r\n    }\r\n  },\r\n  mounted() {\r\n    this.load(1)\r\n\r\n\r\n  },\r\n  // methods：本页面所有的点击事件或者其他函数定义区\r\n  methods: {\r\n    load(pageNum) {\r\n      if (pageNum) this.pageNum = pageNum\r\n      this.$request.get('/lost/selectPage', {\r\n        params: {\r\n          pageNum: this.pageNum,\r\n          pageSize: this.pageSize,\r\n          name: this.name,\r\n        }\r\n      }).then(res => {\r\n        this.lostData = res.data?.list\r\n        this.total = res.data?.total\r\n      })\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      this.pageNum = pageNum\r\n      this.load(this.pageNum)\r\n    },\r\n    reset() {\r\n      this.name = null\r\n      this.load(1)\r\n    },\r\n    viewContent(content) {\r\n      this.viewData = content\r\n      this.viewVisible = true\r\n    },\r\n    contact(item) {\r\n      this.form = JSON.parse(JSON.stringify(item))\r\n      this.fromVisible = true\r\n\r\n    },\r\n    submit() {\r\n      let data = {\r\n        articleId:this.form.id,\r\n        type:'失物广场',\r\n        fromId:this.user.id,\r\n        toId:this.form.userId,\r\n        content:this.content\r\n      }\r\n      this.$request.post('/message/add', data).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success('留言成功，等待对方联系')\r\n        }else{\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n\r\n    }\r\n  }\r\n}\r\n</script>\r\n"],"mappings":"AAiEA,OAAAA,CAAA;AACA;EAEAC,KAAA;IACA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAC,IAAA;MACAC,OAAA;MACAC,QAAA;MACAC,KAAA;MACAC,QAAA;MACAC,WAAA;MACAC,QAAA;MACAC,WAAA;MACAC,OAAA;MACAC,IAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,IAAA;EAGA;EACA;EACAC,OAAA;IACAD,KAAAV,OAAA;MACA,IAAAA,OAAA,OAAAA,OAAA,GAAAA,OAAA;MACA,KAAAY,QAAA,CAAAC,GAAA;QACAC,MAAA;UACAd,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA,QAAA;UACAF,IAAA,OAAAA;QACA;MACA,GAAAgB,IAAA,CAAAC,GAAA;QACA,KAAAb,QAAA,GAAAa,GAAA,CAAAvB,IAAA,EAAAwB,IAAA;QACA,KAAAf,KAAA,GAAAc,GAAA,CAAAvB,IAAA,EAAAS,KAAA;MACA;IACA;IACAgB,oBAAAlB,OAAA;MACA,KAAAA,OAAA,GAAAA,OAAA;MACA,KAAAU,IAAA,MAAAV,OAAA;IACA;IACAmB,MAAA;MACA,KAAApB,IAAA;MACA,KAAAW,IAAA;IACA;IACAU,YAAAb,OAAA;MACA,KAAAF,QAAA,GAAAE,OAAA;MACA,KAAAH,WAAA;IACA;IACAiB,QAAAC,IAAA;MACA,KAAAd,IAAA,GAAAb,IAAA,CAAAC,KAAA,CA