{"ast":null,"code":"export default {\n  data() {\n    return {\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\n      tableData: [],\n      pageNum: 1,\n      pageSize: 5,\n      total: 0,\n      fromVisible: false,\n      form: {}\n    };\n  },\n  mounted() {\n    this.load(1);\n  },\n  // methods：本页面所有的点击事件或者其他函数定义区\n  methods: {\n    load(pageNum) {\n      if (pageNum) this.pageNum = pageNum;\n      this.$request.get('/lost/selectPage', {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize\n        }\n      }).then(res => {\n        this.tableData = res.data?.list;\n        this.total = res.data?.total;\n      });\n    },\n    handleAdd() {\n      this.form = {};\n      this.fromVisible = true;\n    },\n    handleEdit(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n      this.fromVisible = true;\n    },\n    del(id) {\n      this.$confirm('您确定删除吗？', '确认删除', {\n        type: \"warning\"\n      }).then(response => {\n        this.$request.delete('/lost/delete/' + id).then(res => {\n          if (res.code === '200') {\n            // 表示操作成功\n            this.$message.success('操作成功');\n            this.load(1);\n          } else {\n            this.$message.error(res.msg); // 弹出错误的信息\n          }\n        });\n      }).catch(() => {});\n    },\n    save() {\n      this.$request({\n        url: this.form.id ? '/lost/update' : '/lost/add',\n        method: this.form.id ? 'PUT' : 'POST',\n        data: this.form\n      }).then(res => {\n        if (res.code === '200') {\n          // 表示成功保存\n          this.$message.success('保存成功');\n          this.fromVisible = false;\n        } else {\n          this.$message.error(res.msg); // 弹出错误的信息\n        }\n      });\n    },\n\n    handleCurrentChange(pageNum) {\n      this.pageNum = pageNum;\n    },\n    handleImgSuccess(res) {\n      this.form.img = res.data;\n    }\n  }\n};","map":{"version":3,"names":["data","user","JSON","parse","localStorage","getItem","tableData","pageNum","pageSize","total","fromVisible","form","mounted","load","methods","$request","get","params","then","res","list","handleAdd","handleEdit","row","stringify","del","id","$confirm","type","response","delete","code","$message","success","error","msg","catch","save","url","method","handleCurrentChange","handleImgSuccess","img"],"sources":["src/views/front/MyLost.vue"],"sourcesContent":["<template>\r\n  <div class=\"main-content\">\r\n    <div style=\"width: 70%; margin: 0 auto\">\r\n      <div style=\"margin: 20px 0\">\r\n        <el-button type=\"primary\" plain @click=\"handleAdd\">发布失物</el-button>\r\n      </div>\r\n      <div class=\"table\">\r\n        <el-table :data=\"tableData\" stripe  >\r\n          <el-table-column prop=\"id\" label=\"序号\" width=\"80\" align=\"center\" sortable></el-table-column>\r\n          <el-table-column prop=\"img\" label=\"物品主图\" >\r\n            <template v-slot=\"scope\">\r\n              <div style=\"display: flex; align-items: center\">\r\n                <el-image style=\"width: 40px; height: 40px\" v-if=\"scope.row.img\"\r\n                          :src=\"scope.row.img\" :preview-src-list=\"[scope.row.img]\"></el-image>\r\n              </div>\r\n            </template>\r\n          </el-table-column>\r\n          <el-table-column prop=\"name\" label=\"物品名称\" show-overflow-tooltip></el-table-column>\r\n          <el-table-column prop=\"content\" label=\"物品描述\"></el-table-column>\r\n          <el-table-column prop=\"status\" label=\"状态\"></el-table-column>\r\n          <el-table-column prop=\"time\" label=\"时间\"></el-table-column>\r\n\r\n          <el-table-column label=\"操作\" width=\"180\" align=\"center\">\r\n            <template v-slot=\"scope\">\r\n              <el-button plain type=\"primary\" @click=\"handleEdit(scope.row)\" size=\"mini\">编辑</el-button>\r\n              <el-button plain type=\"danger\" size=\"mini\" @click=del(scope.row.id)>删除</el-button>\r\n            </template>\r\n          </el-table-column>\r\n        </el-table>\r\n\r\n        <div class=\"pagination\" style=\"margin-top: 20px\">\r\n          <el-pagination\r\n              background\r\n              @current-change=\"handleCurrentChange\"\r\n              :current-page=\"pageNum\"\r\n              :page-sizes=\"[5, 10, 20]\"\r\n              :page-size=\"pageSize\"\r\n              layout=\"total, prev, pager, next\"\r\n              :total=\"total\">\r\n          </el-pagination>\r\n        </div>\r\n      </div>\r\n\r\n\r\n      <el-dialog title=\"信息\" :visible.sync=\"fromVisible\" width=\"40%\" :close-on-click-modal=\"false\" destroy-on-close>\r\n        <el-form label-width=\"100px\" style=\"padding-right: 50px\" :model=\"form\" >\r\n          <el-form-item prop=\"img\" label=\"物品图片\">\r\n              <el-upload\r\n                  class=\"avatar-uploader\"\r\n                  :action=\"$baseUrl + '/files/upload'\"\r\n                  :headers=\"{ token: user.token }\"\r\n                  list-type=\"picture\"\r\n                  :on-success=\"handleImgSuccess\"\r\n              >\r\n                <el-button type=\"primary\">上传图片</el-button>\r\n              </el-upload>\r\n            </el-form-item>\r\n          <el-form-item prop=\"name\" label=\"物品名称\">\r\n            <el-input v-model=\"form.name\" autocomplete=\"off\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item prop=\"status\" label=\"物品状态\">\r\n            <el-select v-model=\"form.status\" placeholder=\"请选择状态\" style=\"width:100%\">\r\n              <el-option label=\"丢失中\" value=\"丢失中\"></el-option>\r\n              <el-option label=\"已找到\" value=\"已找到\"></el-option>\r\n            </el-select>\r\n            <el-input v-model=\"form.status\" autocomplete=\"off\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item prop=\"content\" label=\"物品描述\">\r\n            <el-input type=\"textarea\" :rows=\"5\" v-model=\"form.content\" autocomplete=\"off\"></el-input>\r\n          </el-form-item>\r\n        </el-form>\r\n        <div slot=\"footer\" class=\"dialog-footer\">\r\n          <el-button @click=\"fromVisible = false\">取 消</el-button>\r\n          <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n        </div>\r\n      </el-dialog>\r\n\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\nexport default {\r\n\r\n  data() {\r\n    return {\r\n      user:JSON.parse(localStorage.getItem('xm-user') || '{}'),\r\n      tableData: [],\r\n      pageNum: 1,\r\n      pageSize: 5,\r\n      total: 0,\r\n      fromVisible: false,\r\n      form: {},\r\n    }\r\n  },\r\n  mounted() {\r\n    this.load(1)\r\n\r\n  },\r\n  // methods：本页面所有的点击事件或者其他函数定义区\r\n  methods: {\r\n    load(pageNum){\r\n      if (pageNum) this.pageNum = pageNum\r\n      this.$request.get('/lost/selectPage', {\r\n        params: {\r\n          pageNum: this.pageNum,\r\n          pageSize: this.pageSize,\r\n        }\r\n      }).then(res => {\r\n        this.tableData = res.data?.list\r\n        this.total = res.data?.total\r\n      })\r\n\r\n    },\r\n    handleAdd(){\r\n      this.form={}\r\n      this.fromVisible=true\r\n\r\n    },\r\n    handleEdit(row){\r\n      this.form=JSON.parse(JSON.stringify(row))\r\n      this.fromVisible=true\r\n    },\r\n    del(id){\r\n      this.$confirm('您确定删除吗？', '确认删除', {type: \"warning\"}).then(response => {\r\n        this.$request.delete('/lost/delete/' + id).then(res => {\r\n          if (res.code === '200') {   // 表示操作成功\r\n            this.$message.success('操作成功')\r\n            this.load(1)\r\n          } else {\r\n            this.$message.error(res.msg)  // 弹出错误的信息\r\n          }\r\n        })\r\n      }).catch(() => {\r\n      })\r\n    },\r\n    save(){\r\n      this.$request({\r\n      url: this.form.id ? '/lost/update' : '/lost/add',\r\n      method: this.form.id ? 'PUT' : 'POST',\r\n      data: this.form\r\n    }).then(res => {\r\n      if (res.code === '200') {  // 表示成功保存\r\n        this.$message.success('保存成功')\r\n        this.fromVisible = false\r\n      } else {\r\n        this.$message.error(res.msg)  // 弹出错误的信息\r\n      }\r\n    })\r\n\r\n    },\r\n    handleCurrentChange(pageNum){\r\n      this.pageNum=pageNum\r\n    },\r\n    handleImgSuccess(res){\r\n      this.form.img=res.data\r\n    }\r\n  }\r\n}\r\n</script>\r\n"],"mappings":"AAmFA;EAEAA,KAAA;IACA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAC,SAAA;MACAC,OAAA;MACAC,QAAA;MACAC,KAAA;MACAC,WAAA;MACAC,IAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,IAAA;EAEA;EACA;EACAC,OAAA;IACAD,KAAAN,OAAA;MACA,IAAAA,OAAA,OAAAA,OAAA,GAAAA,OAAA;MACA,KAAAQ,QAAA,CAAAC,GAAA;QACAC,MAAA;UACAV,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA;QACA;MACA,GAAAU,IAAA,CAAAC,GAAA;QACA,KAAAb,SAAA,GAAAa,GAAA,CAAAnB,IAAA,EAAAoB,IAAA;QACA,KAAAX,KAAA,GAAAU,GAAA,CAAAnB,IAAA,EAAAS,KAAA;MACA;IAEA;IACAY,UAAA;MACA,KAAAV,IAAA;MACA,KAAAD,WAAA;IAEA;IACAY,WAAAC,GAAA;MACA,KAAAZ,IAAA,GAAAT,IAAA,CAAAC,KAAA,CAAAD,IAAA,CAAAsB,SAAA,CAAAD,GAAA;MACA,KAAAb,WAAA;IACA;IACAe,IAAAC,EAAA;MACA,KAAAC,QAAA;QAAAC,IAAA;MAAA,GAAAV,IAAA,CAAAW,QAAA;QACA,KAAAd,QAAA,CAAAe,MAAA,mBAAAJ,EAAA,EAAAR,IAAA,CAAAC,GAAA;UACA,IAAAA,GAAA,CAAAY,IAAA;YAAA;YACA,KAAAC,QAAA,CAAAC,OAAA;YACA,KAAApB,IAAA;UACA;YACA,KAAAmB,QAAA,CAAAE,KAAA,CAAAf,GAAA,CAAAgB,GAAA;UACA;QACA;MACA,GAAAC,KAAA,QACA;IACA;IACAC,KAAA;MACA,KAAAtB,QAAA;QACAuB,GAAA,OAAA3B,IAAA,CAAAe,EAAA;QACAa,MAAA,OAAA5B,IAAA,CAAAe,EAAA;QACA1B,IAAA,OAAAW;MACA,GAAAO,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAY,IAAA;UAAA;UACA,KAAAC,QAAA,CAAAC,OAAA;UACA,KAAAvB,WAAA;QACA;UACA,KAAAsB,QAAA,CAAAE,KAAA,CAAAf,GAAA,CAAAgB,GAAA;QACA;MACA;IAEA;;IACAK,oBAAAjC,OAAA;MACA,KAAAA,OAAA,GAAAA,OAAA;IACA;IACAkC,iBAAAtB,GAAA;MACA,KAAAR,IAAA,CAAA+B,GAAA,GAAAvB,GAAA,CAAAnB,IAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}